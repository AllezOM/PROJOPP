<script>
    document.getElementById("newCommentForm").addEventListener("submit", function(event) {
        event.preventDefault(); // Zapobiegaj domyslnej akcji przesylania formularza

        var authorName = document.getElementById("authorName").value;
        var commentText = document.getElementById("commentText").value;
        var commentsDiv = document.getElementById("comments");

        // Tworzenie nowego elementu div dla komentarza
        var newComment = document.createElement("div");
        newComment.classList.add("comment");

        // Dodanie imienia/nazwy autora
        var authorPara = document.createElement("p");
        authorPara.classList.add("author");
        authorPara.textContent = authorName;
        newComment.appendChild(authorPara);

        // Dodanie tresci komentarza
        var commentPara = document.createElement("p");
        commentPara.textContent = commentText;
        newComment.appendChild(commentPara);

        // Dodanie nowego komentarza do sekcji komentarzy
        commentsDiv.appendChild(newComment);

        // Wyczysc pola po dodaniu komentarza
        document.getElementById("authorName").value = "";
        document.getElementById("commentText").value = "";
    });
</script>

</body>
</html>
